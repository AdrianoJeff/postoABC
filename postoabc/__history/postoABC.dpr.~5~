program postoABC;

uses
  Vcl.Forms,
  untDM in 'untDM.pas' {dm: TDataModule},
  untHome in 'untHome.pas' {frmHome};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(Tdm, dm);
  Application.CreateForm(TfrmHome, frmHome);
  Application.Run;
end.
